<%= content_for :left do %>
  <div class="style_accordion">
               <div class="group">
<ul>All events:
      <% @events.each do |event| %>
      <li><%= link_to event.title, event, remote: true %></li>
      <% end %>

    </ul>
</div>
</div>
<% end %>
<%= content_for :main do %>
  <div id="calendar">
    <%= render 'calendar' %>
</div>

    <%= link_to 'New Event', new_event_path, remote: true, class: 'btn', id: 'event_button' %>
    <div id="current_event">
    </div>

 <% end %>
